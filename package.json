{
    "name": "my-job-aggregator",
    "version": "0.0.1",
    "type": "module",
    "description": "This is an example of a Crawlee project.",
    "dependencies": {
        "crawlee": "^3.0.0",
        "playwright": "*"
    },
    "devDependencies": {
        "@apify/tsconfig": "^0.1.0",
        "tsx": "^4.4.0",
        "typescript": "~5.8.0",
        "@types/node": "^22.0.0"
    },
    "scripts": {
        "start": "npm run start:dev",
        "start:prod": "node dist/main.js",
        "start:dev": "tsx src/main.ts",
        "build": "tsc",
        "test": "jest",
        "prepare-results": "node src/prepare-results.js",
        "view-results": "npm run prepare-results && npx serve",
        "postinstall": "npx crawlee install-playwright-browsers"
    },
